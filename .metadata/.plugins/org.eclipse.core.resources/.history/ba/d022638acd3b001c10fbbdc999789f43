#include"HMI_FUNCTIONS.h"

int main()
{
	int password[5] = {};
	char password_string[6] = {};
	HMI_firstTimePassword(password);
	HMI_convertPasswordToString(password, password_string);
	HMI_sendThePassword(password_string);
}








